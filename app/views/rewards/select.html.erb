<script type="text/javascript">
	function formElementBlur(target, hint, isPassword) {
		if (target.value == '') {
			target.value = hint;
			if ((isPassword == true)) {
				target.type = "text"
			}
		}
	}

	function formElementFocus(target, hint, isPassword) {
		if (target.value == hint) {
			target.value = '';
		}
		if (isPassword == true) {
			target.type = "password"
		}
	}

	function textElementBlur(target, hint) {
		if ($(target).html() == '') {
			$(target).html(hint);
		}
	}

	function textElementFocus(target, hint) {
		if ($(target).html() == hint) {
			$(target).html("");
		}
	}

	<% if @early_bird %>
	$(document).ready(function () {
		$('#check_early_bird').submit(function () {
			$("#check_message").hide();
			if (["linq4", "linq5", "linq6", "linq7", "linq12", "linq24", "linqlb", "linqtc", "linqmash", "linqgo", "linqgm", "linqeng"].indexOf($('#early_bird_code').val()) >= 0) {
				$("#pledge_tier_1").removeAttr('disabled');
				$('#check_message').text("Early bird option is unlocked! You can select that option now").show();
			} else {
				$('#check_message').text("Not an early bird code.").show();
			}
			return false;
		});
	});
	<% end %>

</script>

