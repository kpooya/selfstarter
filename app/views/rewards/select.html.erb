<script type="text/javascript">
	jQuery(document).ready(function ($) {
		$('#toggle_distributor').click(function () {
			$('#distributor_wrapper').toggle(100);
			if ($('#toggle_distributor').text() == 'Reserve as a Distributor ▼') {
				$('#toggle_distributor').text('Reserve as a Distributor ▲');
			} else {
				$('#toggle_distributor').text('Reserve as a Distributor ▼');
			}
		})

		<% if request.params[:d] %>
			$('#toggle_distributor').click();
		<% end %>
		})

		$('#distributorForm').submit(function () {
			$('#distributor_wrapper').toggle(100);
			<%# TODO, high priority %>
			$('#distributorErrorInfo').html("Thanks! We will contact you shortly.").show();
			return false;
		});
</script>
