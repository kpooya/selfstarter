<script>
    $(document).ready(function () {
	    $("#same_as_my_shipping_address").change(function(){
		    if ($('#same_as_my_shipping_address').is(':checked')) {
			    $("#billing_address").hide();
		    } else {
			    $("#billing_address").show();
		    }
	    })

        Stripe.setPublishableKey("<%=STRIPE_PUBLISHABLE_KEY %>");
        var subscription = {
            setupForm:function () {
                $('#new_pledge').submit(function () {
                    $('input[type=submit]').prop('disabled', true);
                    if ($('#card_number').length) {
                        subscription.processCard();
                        return false;
                    } else {
                        return true;
                    }
                });
            },
            processCard:function () {
                var card;
                card = {
                    name:$('#order_user_name').val(),
                    number:$('#card_number').val(),
                    cvc:$('#card_code').val(),
                    expMonth:$('#card_month').val(),
                    expYear:$('#card_year').val()
                };
                Stripe.createToken(card, subscription.handleStripeResponse);
            },
            handleStripeResponse:function (status, response) {
                if (status === 200) {
                    $('#user_stripe_token').val(response.id);
                    <%= if Rails.env.development? ; 'console.log(response.id)' ; end %>

                    $('#new_pledge')[0].submit();
                } else {
                    $('#stripe_error').text(response.error.message).show();
                    return $('input[type=submit]').prop('disabled', false);
                }
            }
        };
        subscription.setupForm();
    });
</script>

