            <label for="pledge_email">Email</label>
            <% if Rails.env.development? ; user_email = 'qiang8701+' + Random.new.rand(10000).to_s + '@gmail.com' ; end %>

            <input class="very-long rounded no-ajax" id="pledge_email" name="user[email]" size="30" type="text"
